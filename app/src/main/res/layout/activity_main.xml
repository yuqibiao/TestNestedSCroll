<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_dispatch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TEST_DISPATCH_EVENT"/>

    <Button
        android:id="@+id/btn_coo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="TEST_COORDINATOR"/>

    <Button
        android:id="@+id/btn_collapse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="TEST_COO_COLLAPSE"/>

    <Button
        android:id="@+id/btn_nested"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="TEST_NESTED"/>

    <Button
        android:id="@+id/btn_line_chart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="TEST_LINE_CHART"/>

</LinearLayout>
